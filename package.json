{
  "name": "ep-task",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "concurrently  \"node-sass src/css -o src/css\" \"postcss src/css/site.css -o assets/css/site.css\"",
    "watch": "concurrently  \"node-sass --watch src/css -o src/css\" \"postcss src/css/site.css -o assets/css/site.css --watch\""
  },
  "repository": "git@github.com:JOgonowski/ep-task.git",
  "author": "Jacek Ogonowski <jacek.ogonowski@considonet.com>",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0"
  },
  "browserslist": [
    "ie >= 9",
    "> 1%",
    "iOS 8"
  ]
}
